{
  "name": "parcel-boilerplate",
  "description": "A parcel boilerplate for no-framework front-end",
  "version": "1.0.0",
  "main": "dist/index.html",
  "browserslist": "> 0.25%",
  "scripts": {
    "dev": "rm -rf ./developement && rm -rf ./.parcel-cache && parcel serve ./src/html/index.html --dist-dir development -p 3000",
    "build": "rm -rf ./dist && parcel build ./src/html/index.html --dist-dir dist --public-url ./"
  },
  "keywords": [],
  "author": "VincentLrg",
  "license": "ISC",
  "engines": {
    "node": ">= 12.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/runtime-corejs2": "^7.13.17",
    "@parcel/transformer-image": "^2.0.0-beta.2",
    "@parcel/transformer-sass": "^2.0.0-beta.2",
    "parcel": "^2.0.0-beta.2",
    "sass": "^1.32.11"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "firebase": "^8.4.2",
    "gsap": "^3.6.1",
    "pixi.js": "^6.0.2"
  }
}
